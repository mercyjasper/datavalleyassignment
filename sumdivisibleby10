public class SumDivisibleBy10 {

    public static void main(String[] args) {
        int[] values = {100, 52, 43, 99, 42, 40, 91};
        int sum = 0;

        // Iterate through the array
        for (int num : values) {
            // Check if the number is divisible by 10 (divisible by both 5 and 2)
            if (num % 10 == 0) {
                sum += num;
            }
        }

        // Print the sum
        System.out.println("Sum of numbers divisible by 10: " + sum);
    }
}
